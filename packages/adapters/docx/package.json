{
  "name": "html-to-document-adapter-docx",
  "version": "0.4.2",
  "description": "DOCX adapter for html-to-document-core â€” converts a DocumentElement tree into a .docx Buffer using the docx library.",
  "keywords": [
    "html-to-document",
    "docx",
    "adapter",
    "docx-converter"
  ],
  "license": "ISC",
  "homepage": "https://github.com/ChipiKaf/html-to-document#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChipiKaf/html-to-document.git",
    "directory": "packages/adapters/docx"
  },
  "bugs": {
    "url": "https://github.com/ChipiKaf/html-to-document/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.js"
      }
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json",
    "test": "vitest --watch false",
    "test:watch": "vitest --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}' --ignore-pattern '**/*.d.ts'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'"
  },
  "dependencies": {
    "docx": "^9.4.0",
    "image-size": "^2.0.2",
    "jszip": "^3.10.1"
  },
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "html-to-document-core": "workspace:*"
  },
  "devDependencies": {
    "@types/probe-image-size": "^7.2.5",
    "html-to-document-core": "workspace:^",
    "ts-toolbelt": "^9.6.0",
    "vitest": "^3.2.4"
  }
}
